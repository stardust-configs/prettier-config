{
  "name": "@stardust-configs/prettier-config",
  "version": "0.1.1",
  "description": "Shareable Prettier config",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "homepage": "https://github.com/stardust-configs/prettier-config#readme",
  "bugs": "https://github.com/stardust-configs/prettier-config/issues",
  "license": "MIT",
  "author": "P-Chan",
  "files": [
    ".prettierrc.json"
  ],
  "main": ".prettierrc.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/stardust-configs/prettier-config.git"
  },
  "scripts": {
    "validate": "jsv \".prettierrc.json\" --schema \"https://json.schemastore.org/prettierrc\"",
    "version:major": "standard-version -r major",
    "version:minor": "standard-version -r minor",
    "version:patch": "standard-version -r patch"
  },
  "devDependencies": {
    "@stardust-configs/json-schema-validator": "0.4.0",
    "standard-version": "9.5.0"
  },
  "peerDependencies": {
    "prettier": "^2.0.0"
  }
}
