{
  "name": "@stardust-configs/prettier-config",
  "version": "0.1.1",
  "description": "Shareable Prettier config",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "homepage": "https://github.com/stardust-configs/prettier-config#readme",
  "bugs": "https://github.com/stardust-configs/prettier-config/issues",
  "license": "MIT",
  "author": "P-Chan",
  "files": [
    ".prettierrc.json"
  ],
  "main": ".prettierrc.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/stardust-configs/prettier-config.git"
  },
  "scripts": {
    "validate": "jsv \".prettierrc.json\" --schema \"https://json.schemastore.org/prettierrc\"",
    "version": "standard-version",
    "release": "dotenv -- conventional-github-releaser -p angular"
  },
  "devDependencies": {
    "@stardust-configs/json-schema-validator": "^0.1.2",
    "consola": "^2.15.0",
    "conventional-github-releaser": "^3.1.5",
    "dotenv-cli": "^4.0.0",
    "standard-version": "^9.1.0"
  },
  "peerDependencies": {
    "prettier": "^2.0.0"
  }
}
